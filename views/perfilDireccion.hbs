   <div class="formulario contact">
	<div class="row">
		<div class="col-md-3" >
			<div class="contact-info" >
				<img src="https://i0.wp.com/mundowin.com/wp-content/uploads/2019/06/Your-Location-has-Recently-Been-Accessed-windows-10.png?w=832&ssl=1"  style="padding-top: 35%; padding-bottom:35% ;"  height="350px" width="200px">

				
			</div>
		</div>
		
		<div class="col-md-9">
		 <form  action="/miperfil-direccion" method="post" id="registrationForm">
                     <div class="form-group">
                          
                          <div class="col-xs-6">
                              <label for="email"><h4 style="color: floralwhite;">Agrega una nueva dirección:</h4></label>
                            
                              <textarea type="text" name="direccion" placeholder="Dirección"> </textarea>
                          </div>
                      </div>
                      <div class="form-group">
                          
                          <div class="col-xs-6">
                              <label for="first_name"><h4 style="color: floralwhite;">DIRECCIONES</h4></label>
                          </div>
                      </div> 
                  
              
  <div class="table-wrapper-scroll-y my-custom-scrollbar">    
  <table class="table  table-dark   textarea  table-striped mb-0 ">
     <div class="tbl-header">
    
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Dirección</th>
    </tr>
  </thead>
  </div>
  <tbody>
    {{#each direcciones as |item i| }}
    <tr>
      <th scope="row">{{i}}</th>
      <th scope="row">{{this}}</th>
      <th>

</th>
       {{else}}
     <th></th>  
     <th>Aún no se han agregado direcciones.</th>
    </tr>
    {{/each}}
  </tbody>
</table>
</div> 
                      
                      <div class="form-group">
                           <div class="col-xs-12">
                                <br>
                              	<button class="btn btn-lg btn-primary pull-right" type="submit"><i class="glyphicon glyphicon-ok-sign"></i> Agregar</button>
