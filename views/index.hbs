<!--cuarpo del body-->
<link rel="stylesheet" href="estilos.css">
<link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css'>
<div class="">
  <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block" src="https://growproexperience.com/wp-content/uploads/2015/10/Trabajar-en-construccio%CC%81n-en-Australia.jpg
" alt="First slide" style="filter: blur(1px)">
     <div class="carousel-caption">
                     <h2 style="font-size: 50px;"  class="animated fadeInLeft deepshadow">Convertimos tus necesidades en nuestras prioridades</h2>
                   
                   
                </div>
    </div>
    <div class="carousel-item">
      <img class="d-block" src="https://estaticos.qdq.com/swdata/home_photos/862/862339402/213e407d0cf442719bc2ba54c7a295bb.jpg
" alt="Second slide">
      
     <div class="carousel-caption">
                     <h2 style="font-size: 60px; background-color:  rgba(43, 38, 38, 0.4);" class="animated fadeInDown deepshadow">Unete a nuestro equipo </h2>
                     <p style="font-size: 30px;" class="animated fadeInUp">¡Y se parte de una nueva forma de brindar servicios!</p>
                    
                </div>
    </div>
    <div class="carousel-item">
      <img class="d-block" src="/img/robert.jpg" alt="Third slide" >
     <div class="carousel-caption">
                     <h2 style="font-size: 60px;" class="animated fadeInRight deepshadow">Sinulle es para ti</h2>
                     <p style="font-size: 30px;" class="animated fadeInRight">Porque pensamos en ti y en la mejor forma de hacer las cosas</p>
                     
                </div>
                
    </div>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
  
</div> 
{{!-- Inicio de categorias --}}
<nav class="navbars navbar-expand-lg navbar-dark">
<p class="servicios insetshadow animated fadeInLeft deepshadow">Solicita nuestros servicios</p>
</nav>
{{!-- Fin de categorias --}}
{{!-- incio de grid --}}

 <section class="grid ">
  
	  	<div class="contenedor-cards">     
 {{#each servicios}}
		    <div class="contenedor-card-item">
		      	<div class="contenedor-card-item-wrapper">
		        	<a  href="#"> <img  class="card-img-top" src="{{imagen}}" alt=""></a>
             
		        	<div class="contenedor-info">
		          		<div class="info col-md-12">
                    <div class="row-10">
                      <p class="titulo">{{nombre}}</p>
                    </div>
                     <div class="row-2">
                         <a href="/crear-orden/{{url}}" class=" icon-2x text-warning stretched-link ">Solicitar</a></span>
                      </div>          	
                  
		          		</div>

		          		<div class="fondo"></div>
		        	</div>
		      	</div>
            
		    </div>
        	 
		    {{/each}}
	   	</div>
</section>
 
{{!-- fin del grid --}}
